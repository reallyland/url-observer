{
  "name": "url-observer",
  "version": "0.0.0",
  "private": true,
  "description": "URLObserver observes URL changes in a web browser",
  "keywords": [
    "url-observer",
    "lit-element",
    "lit-html",
    "polymer",
    "typescript",
    "web-component",
    "web-components"
  ],
  "homepage": "https://github.com/reallyland/url-observer",
  "repository": {
    "type": "git",
    "url": "git@github.com:reallyland/url-observer.git"
  },
  "license": "MIT",
  "author": {
    "name": "Rong Sen Ng",
    "email": "wes.ngrongsen@gmail.com",
    "url": "https://github.com/motss"
  },
  "main": "./dist/my-element.js",
  "module": "./dist/my-element.js",
  "typings": "./dist/my-element.d.ts",
  "files": [
    "dist/*.d.ts.map",
    "dist/*.d.ts",
    "dist/*.js.map",
    "dist/*.js"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.prod.json",
    "build:config": "tsc -p tsconfig.wdio.json",
    "clean": "shx rm -rf .*cache *.log dist/ logs/",
    "lint": "npm run lint:debug -- --config tslint.prod.json",
    "lint:debug": "tslint --project tsconfig.json --format stylish",
    "prepublishOnly": "npm run clean && npm run lint && npm run build",
    "serve": "es-dev-server --app-index=demo.html --node-resolve --port=3000 --watch",
    "serve:compile": "es-dev-server --app-index=demo.html --node-resolve --port=3000 --watch --babel --babel-config=babel.config.json",
    "test": "npm run build && npm run test:dev",
    "test:dev": "node --trace-deprecation run-server.js --config-file dist/tests/wdio.config.js",
    "test:sl:dev": "env-cmd -f ./.env node --trace-deprecation run-server.js --config-file dist/tests/wdio.sl.config.js",
    "ts": "tsc",
    "watch": "npm run clean && tsc --watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && tsc --noEmit"
    }
  },
  "dependencies": {
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.10.2",
    "@babel/register": "^7.10.1",
    "@material/mwc-button": "^0.15.0",
    "@material/mwc-dialog": "^0.15.0",
    "@reallyland/tsconfig": "^2.0.0",
    "@reallyland/tslint-config": "^1.1.1",
    "@types/mocha": "^7.0.2",
    "@wdio/cli": "^6.1.16",
    "@wdio/local-runner": "^6.1.16",
    "@wdio/mocha-framework": "^6.1.14",
    "@wdio/sauce-service": "^6.1.16",
    "@wdio/selenium-standalone-service": "^6.1.14",
    "@wdio/spec-reporter": "^6.1.14",
    "env-cmd": "^10.1.0",
    "es-dev-server": "^1.54.0",
    "expect-webdriverio": "^1.1.5",
    "husky": "^4.2.3",
    "lit-element": "^2.3.1",
    "lit-html": "^1.2.1",
    "path-to-regexp": "^6.1.0",
    "shx": "^0.3.2",
    "tslint": "^6.1.0",
    "typescript": "^3.9.3",
    "webdriverio": "^6.1.16"
  },
  "engines": {
    "node": ">= 10.18.1",
    "npm": ">= 6.13.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
